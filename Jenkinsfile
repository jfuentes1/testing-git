pipeline{
    agent any

    stages {
        
        stage("build"){
            
            steps {
                echo 'compilando la app..'
            }
        }

        stage("test"){
            
            steps {
                echo 'probando la app..'
            }
        }

        stage("deploy"){
            
            steps {
                echo 'desplegando la app..'
            }
        }
    }
}
